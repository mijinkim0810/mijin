--등록 테이블
create table REGISTRATION(
R_NO number(6) not null,
R_DATE DATE NOT NULL,
R_GRADUATIONDATE  DATE NOT NULL,
R_CLASSASSIONMENT VARCHAR2(10) NOT NULL,
FILENAME VARCHAR2(100), 
PRIMARY KEY(R_NO),
K_CLASS VARCHAR2(10) NOT NULL REFERENCES KINDERGARDEN(K_CLASS),
S_NO NUMBER(20) NOT NULL REFERENCES STUDENT(S_NO)
); //외래키는 별도의 옵션이 없으므로 delete,update시 제약(restrict)이 있다

SELECT *
FROM REGISTRATION;

--등록 테이블 시퀀스 생성
CREATE SEQUENCE REGISTRATION_SEQ
START WITH 0
INCREMENT BY 1
MAXVALUE 999999
MINVALUE 0
NOCYCLE
CACHE 2;
--유치원생 테이블
CREATE TABLE STUDENT(
S_NO NUMBER(19) NOT NULL,
S_NAME VARCHAR2(10) NOT NULL,
S_GENDER VARCHAR2(2) NOT NULL,
S_BLOOD VARCHAR2(3) NOT NULL,
S_RRN VARCHAR2(20) NOT NULL,
S_DISEASE VARCHAR2(10) NOT NULL,
S_ADDRESS VARCHAR2(30) NOT NULL,
S_TUITION NUMBER(7) NOT NULL,
S_NATIOALDISCOUNT VARCHAR2(5) NOT NULL,
S_BRINGER VARCHAR2(10) NOT NULL,
S_DISCOUNT NUMBER(5) NOT NULL,
S_BROSIS VARCHAR(5) NOT NULL,
S_PATMENT VARCHAR2(5) NOT NULL,
S_PRRN VARCHAR2(20) NOT NULL,
S_PNAME VARCHAR2(10) NOT NULL,
S_PGENDER VARCHAR2(2) NOT NULL,
S_PJOB VARCHAR2(10) NOT NULL,
S_PWORKINGTIME VARCHAR2(10) NOT NULL,
S_PPHONENUMBER VARCHAR2(20) NOT NULL,
PRIMARY KEY (S_NO)
);
--유치원생 테이블 시퀀스 생성
CREATE SEQUENCE CHILDREN_SEQ
START WITH 0
INCREMENT BY 1
MAXVALUE 999999
MINVALUE 0
NOCYCLE
CACHE 2;
--유치원테이블
CREATE TABLE KINDERGARDEN(
K_CLASS VARCHAR2(10) NOT NULL,
K_PEOPLE NUMBER(20) NOT NULL,
K_FLOORS NUMBER(4) NOT NULL,
K_NO NUMBER(4) NOT NULL,
PRIMARY KEY(K_CLASS)
);
--유치원테이블 시퀀스 생성
CREATE SEQUENCE KINDERGARDEN_SEQ
START WITH 0
INCREMENT BY 1
MAXVALUE 999999
MINVALUE 0
NOCYCLE
CACHE 2;

--근무테이블
CREATE TABLE WORK(
W_EHEALTH VARCHAR2(15) NOT NULL,
W_EART VARCHAR2(15) NOT NULL,
W_ENATURE VARCHAR2(15) NOT NULL,
W_ECOMMUNICATION VARCHAR2(15) NOT NULL,
W_SOCIAL VARCHAR2(15) NOT NULL,
W_NO NUMBER(7) NOT NULL,
W_CONSULTRATION VARCHAR2(5) NOT NULL,
W_STUDENT VARCHAR2(10) NOT NULL,
W_CARNUMBER NUMBER NOT NULL,
W_SALARY NUMBER NOT NULL,
W_EPHYSICALDEVELOP VARCHAR2(20) NOT NULL,
W_EEDUCATIONALDEVELOP VARCHAR2(20) NOT NULL,
PRIMARY KEY(W_NO),
S_NO NUMBER(20) NOT NULL REFERENCES STUDENT(S_NO),
T_NO NUMBER(12) NOT NULL REFERENCES TEACHER(T_NO)
);
--근무 시퀀스
CREATE SEQUENCE WORK_SEQ
START WITH 0
INCREMENT BY 1
MAXVALUE 999999
MINVALUE 0
NOCYCLE
CACHE 2;
--선생님 테이블
CREATE TABLE TEACHER(
T_NAME VARCHAR2(10) NOT NULL,
T_GENDER VARCHAR2(2) NOT NULL,
T_RANK VARCHAR2(10) NOT NULL,
T_PRENUMBER NUMBER(10) NOT NULL,
T_BANKNAME VARCHAR2(10) NOT NULL,
T_ACCOUNTNUMBER VARCHAR2(20) NOT NULL,
T_RRN VARCHAR2(20) NOT NULL,
T_HIGHCOLLEGE VARCHAR2(15) NOT NULL,
T_CRETIFICATION VARCHAR2(5) NOT NULL,
T_ENTERINGDATE DATE NOT NULL,
T_NO NUMBER(12) NOT NULL,
T_PHONE VARCHAR2(15) NOT NULL,
PRIMARY KEY(T_NO)
);
--선생님테이블 시퀀스
CREATE SEQUENCE TEACHER_SEQ
START WITH 0
INCREMENT BY 1
MAXVALUE 999999
MINVALUE 0
NOCYCLE
CACHE 2;


